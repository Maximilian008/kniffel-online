# Environment configuration for Kniffel Online
PORT=3000
NODE_ENV=development
ALLOWED_ORIGINS=http://localhost:5173

# Invite token signing (change in production)
INVITE_SECRET=local-dev-invite-secret
INVITE_TTL_SECONDS=3600

# Backend base URL (leer = gleicher Origin)
VITE_API_BASE=

# Room adapter: mock | http  (http erfordert laufende /api/rooms* Endpoints)
VITE_ROOM_ADAPTER=mock
